<h1>Listing events</h1>

<table>
  <tr>
    <th>ID</th>
    <th>Google_ID</th>
    <th>G created</th>
    <th>G updated</th>
    <th>Summary</th>
    <th>Location</th>
    <th>Start</th>
    <th>End</th>
    <th>Timezone</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @events.each do |event| %>
  <tr>
    <td><%= event.id %></td>
    <td><%= event.google_id %></td>
    <td><%= event.g_created %></td>
    <td><%= event.g_updated %></td>
    <td><%= event.summary %></td>
    <td><%= event.location %></td>
    <td><%= event.start %></td>
    <td><%= event.end %></td>
    <td><%= event.timezone %></td>
    <td><%= event.latitude %></td>
    <td><%= event.longitude %></td>
    <td><%= event.user_id %></td>
    <td><%= link_to 'Show', event %></td>
    <td><%= link_to 'Edit', edit_event_path(event) %></td>
    <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Event', new_event_path %>
