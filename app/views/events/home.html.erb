<% content_for :page_scripts do %>
  <script type="text/javascript" src="http://maps.google.com/maps/api/js?key=AIzaSyBxxlbKadojUc5YHoKY2XiuO8d3RIZYths&sensor=false"></script>
  <%= render 'calcroute_setup', :format => [:js], :events => @events %>
  <style type="text/css">
  #mapContainer {
    height: 250px;
    width: 280px;
    /*border:5px solid #eaeaea;*/
  }

  </style>
<% end -%>

<div class="wrapper">
  <div class="content">
    <div class="daynav">
      <img src="/assets/logo.png">
        <ul>
          <%= link_to refresh_events_path do %>
            <li is "refreshbutton"><img src="/assets/refresh.png" id="refresh"></li> 
          <% end %>
        </ul>
    </div><!--/daynav-->
    <div class="progress"></div>
    <%= render 'events', :events => @events %>
    </br>
    <%= render 'mapform' %>
    <br>
    <div class="map" id="mapContainer"></div>
    <div id="directionsPanel"></div>
  </div><!--/content-->
    
</div><!--/wrapper-->

