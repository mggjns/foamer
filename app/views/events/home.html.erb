<style type="text/css">
#mapContainer {
    height: 250px;
    width: 280px;
    /*border:5px solid #eaeaea;*/
}

</style>

<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>
<script type="text/javascript" src="/assets/home.js"></script>



<div class="wrapper">
    
    <% @events.each do |event| %>

        <script type="text/javascript">
            $(document).ready(function() {
                $(initialize());
                $(calcRoute());
            });
        </script>

        <div class="arrowleft"></div>
        <div class="content">
        
        <%= render 'calcroute.js', :event => event %>
      

        <%= render 'events', :event => event %>
        <%= paginate @events %>

        <div class="map" id="mapContainer">
          <!-- <img src="direction.png"> -->
        </div><!--/fullday-->
        
        <div id="directionsPanel">
        
        </div>

        </div><!--/content-->

        <div class="arrowright"></div>
    
    <% end %>  

</div><!--/wrapper-->

<%#= debug event %>
<%#= binding.pry %>
<%#= debug @api_data %>
<%#= debug current_user.timezone if current_user %>
<%#= debug params[:page] %>
