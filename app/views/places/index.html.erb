<h1>Listing places</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Desc</th>
    <th>Street</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @places.each do |place| %>
  <tr>
    <td><%= place.name %></td>
    <td><%= place.desc %></td>
    <td><%= place.street %></td>
    <td><%= place.city %></td>
    <td><%= place.state %></td>
    <td><%= place.zip %></td>
    <td><%= place.user_id %></td>
    <td><%= link_to 'Show', place %></td>
    <td><%= link_to 'Edit', edit_place_path(place) %></td>
    <td><%= link_to 'Destroy', place, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Place', new_place_path %>
